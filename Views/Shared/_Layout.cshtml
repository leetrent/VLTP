<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - VLTP</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class="urs-outer-container ">
        <nav class="navbar navbar-light navbar-expand-lg urs-navbar">
            <a class="navbar-brand urs-navbar-brand" href="#">
                <img src="~/images/nav-logo.png" width="30" height="30" alt="GSA Logo">
            </a>
            <button aria-controls="navbarCollapse"
                    aria-expanded="false"
                    aria-label="Toggle navigation"
                    class="navbar-toggler"
                    data-target="#topNavbarCollapse"
                    data-toggle="collapse"
                    type="button">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="topNavbarCollapse">
                <ul class="mr-auto navbar-nav">
                    <li class="nav-item"><a class="nav-link urs-nav-link-brand" <a asp-area="" asp-controller="Home" asp-action="Index">Voluntary Leave Transfer Program</a></li>
                </ul>
                <ul class="footer-links">
                    <li>
                        <a class="urs-link" asp-area="" asp-controller="Home" asp-action="Faq">FAQ</a>
                    </li>
                    <li>|</li>
                    <li>
                        <a class="urs-link" href="~/pdf/UserGuide.pdf" target="_blank">User Guide</a>
                    </li>
                    <li>|</li>
                    <li>
                        <a class="urs-link" asp-area="" asp-controller="Home" asp-action="Help">Help</a>
                    </li>
                    <li>|</li>
                    <li>
                        <a class="urs-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li>|</li>
                    <li style="margin-right: 15px;">
                        <a class="urs-link" asp-area="" asp-controller="Home" asp-action="Index">Login</a>
                    </li>
                </ul>
            </div>
        </nav>
        <div class="container body-content urs-body-content">
            @RenderBody()
        </div>
        <footer>
            <div class="footer-outer-div">
                <div class="footer-inner-div-left">
                    <h3 title="GSA Logo" class="footer-logo">U.S. General Services Administration</h3>
                </div>
                <div class="footer-inner-div-right">
                    <span class="pull-right">
                        <img alt="American Flag" src="https://www.gsa.gov/sites/gsa.gov/templates/resources/images/icn-us-flag-21px.png" class="display-inline" /><small>&nbsp;Official website of the U.S. Government</small>
                    </span>
                </div>
            </div>
        </footer>
        <!-- Version: [VLTP][3.1][Thursday, October 15, 2020][03:30 PM CT] -->
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
